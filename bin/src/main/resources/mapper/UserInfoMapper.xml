<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="com.example.demo.UserInfoMapper" >
    <select id="UserInfoList" resultType="com.example.demo.UserInfoDto">
        select * from user_info
    </select>
    
    <select id="UserInfo" parameterType="string" resultType="com.example.demo.UserInfoDto">
        select * from user_info where user_id = #{id}
    </select>




</mapper>

